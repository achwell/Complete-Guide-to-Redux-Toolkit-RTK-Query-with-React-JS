{
  "name": "vite-react-template-redux",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "download-schema": "npx apollo service:download --endpoint https://anilist.co/graphql ./src/app/graphql/graphql-schema.json",
    "schema:generate-watch": "npx apollo codegen:generate --localSchemaFile=./src/app/graphql/graphql-schema.json --target=typescript --tagName=gql --watch"  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@reduxjs/toolkit": "^1.8.5",
    "graphql": "15.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "redux": "^4.2.0",
    "reselect": "^4.1.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.1.0",
    "apollo": "^2.34.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.4"
  }
}
